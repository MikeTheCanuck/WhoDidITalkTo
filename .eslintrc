{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "strict": ["error", "global"],
      "curly": "warn",
      "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }]
    }
}